<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/web_drawer_layout"
    tools:context="cn.ipathology.dp.activity.detail.AcdSeeActivity">

    <RelativeLayout
        android:id="@+id/acd_activity_acdsee"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <cn.ipathology.dp.view.AutoSwipeRefreshLayout
            android:id="@+id/acd_see_refresh"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.github.lzyzsd.jsbridge.BridgeWebView
                android:id="@+id/acd_see_webView"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </com.github.lzyzsd.jsbridge.BridgeWebView>
        </cn.ipathology.dp.view.AutoSwipeRefreshLayout>


        <TextView
            android:id="@+id/one_top"
            android:layout_width="12dp"
            android:layout_height="3dp"
            android:layout_marginTop="75dp"
            android:background="@color/normal"
            android:visibility="gone" />

        <TextView
            android:id="@+id/one_bottom"
            android:layout_width="3dp"
            android:layout_height="12dp"
            android:layout_marginTop="75dp"
            android:background="@color/normal"
            android:visibility="gone" />

        <TextView
            android:id="@+id/two_bottom"
            android:layout_width="3dp"
            android:layout_height="12dp"
            android:background="@color/normal"
            android:visibility="gone" />

        <TextView
            android:id="@+id/two_top"
            android:layout_width="12dp"
            android:layout_height="3dp"
            android:background="@color/normal"
            android:visibility="gone" />

        <TextView
            android:id="@+id/three_bottom"
            android:layout_width="3dp"
            android:layout_height="12dp"
            android:layout_marginTop="300dp"
            android:background="@color/normal"
            android:visibility="gone" />

        <TextView
            android:id="@+id/three_top"
            android:layout_width="12dp"
            android:layout_height="3dp"
            android:layout_marginTop="300dp"
            android:background="@color/normal"
            android:visibility="gone" />

        <TextView
            android:id="@+id/four_bottom"
            android:layout_width="3dp"
            android:layout_height="12dp"
            android:layout_marginTop="350dp"
            android:background="@color/normal"
            android:visibility="gone" />

        <TextView
            android:id="@+id/four_top"
            android:layout_width="12dp"
            android:layout_height="3dp"
            android:layout_marginTop="350dp"
            android:background="@color/normal"
            android:visibility="gone" />


        <TextView
            android:id="@+id/acd_frame_top"
            android:layout_width="match_parent"
            android:layout_height="50px"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="75dp" />

        <TextView
            android:id="@+id/acd_frame_bottom"
            android:layout_width="match_parent"
            android:layout_height="50px"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="75dp" />


    </RelativeLayout>

    <ListView
        android:id="@+id/acd_slide_bar"
        android:background="#2F2F31"
        android:layout_width="165dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:layout_height="match_parent"
        android:scrollbars="none"
        android:layout_gravity="right">
    </ListView>


</android.support.v4.widget.DrawerLayout>
